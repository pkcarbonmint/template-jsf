{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "The user's full name"
      },
      "age": {
        "type": "integer",
        "minimum": 0,
        "description": "The user's age in years"
      }
    },
    "required": ["name", "age"]
  }