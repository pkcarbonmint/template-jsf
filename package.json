{
  "name": "template-jsf",
  "version": "1.0.0",
  "main": "dist/runtime.umd.js",
  "module": "dist/runtime.es.js",
  "types": "dist/runtime.d.ts",
  "bin": {
    "generate-template": "bin/generate-template.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:cli": "tsc -p tsconfig.cli.json",
    "build:css": "npx sass src/styles/templateMapping.scss dist/templateMapping.intermediate.css && tailwindcss -i dist/templateMapping.intermediate.css -o dist/jsf.css",
    "compile-templates": "ts-node src/tools/template-compiler.ts",
    "serve": "vite preview",
    "lint": "eslint src --ext .ts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:generator": "node tests/generator-test.js",
    "test:runtime": "node tests/runtime-test.js",
    "test:runtime:verbose": "node tests/runtime-test.js --verbose",
    "test:runtime:vv": "node tests/runtime-test.js --very-verbose",
    "generate-template": "ts-node src/template-generator/index.ts",
    "batch-generate": "node batchGenerate.js",
    "generate-all": "bin/generate-all.js",
    "generate:minify": "node bin/generate-all.js --minify",
    "test:layout-spec": "node run-layout-spec-tests.js",
    "test:jest:generator": "node src/template-generator/tests/run-jest-tests.js generator",
    "test:jest:runtime": "node src/template-generator/tests/run-jest-tests.js runtime",
    "test:jest:layout": "node src/template-generator/tests/run-jest-tests.js layout",
    "test:jest:all": "node src/template-generator/tests/run-jest-tests.js --all",
    "test:jest:validation": "node src/template-generator/tests/run-jest-tests.js -- --testPathPattern=validation.test.js",
    "start": "node src/playground/index.js",
    "generate": "node bin/generate-template.js",
    "playground:start": "ts-node src/playground/app.ts",
    "playground:dev": "nodemon --watch src/playground --ext ts --exec ts-node src/playground/app.ts"
  },
  "keywords": [
    "json-schema",
    "forms",
    "template-generator",
    "playground"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stoplight/json-schema-tree": "^1.0.0",
    "@types/lodash": "^4.17.16",
    "@types/mustache": "^4.2.5",
    "ajv": "^8.12.0",
    "body-parser": "^1.20.2",
    "codemirror": "^5.65.12",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fp-ts": "^2.16.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "tailwindcss": "^3.4.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.26.10",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jquery": "^3.5.16",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "chai": "^4.3.7",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "live-server": "^1.2.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.86.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "^5.2.6"
  },
  "description": "Custom JSF component for AHML",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.0.0"
}
